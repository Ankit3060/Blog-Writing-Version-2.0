<%- include('partials/header'); -%>



<h1>All Composed Posts</h1>

<div class="posts-list">
  <% posts.forEach(function(post) { %>
    <div class="post">
      <h2><%= post.title %></h2>
      <p><%= post.content.substring(0, 100) + " ..."%>
        <a href="/posts/<%=post._id%>">Read More</a></p>
    </div>
  <% }); %>
</div>


<%- include('partials/footer'); -%> 